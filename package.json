{
  "name": "Boiler",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.4.4",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "jest": "^23.6.0",
    "jest-react-native": "^18.0.0",
    "jetifier": "^1.6.3",
    "react-test-renderer": "16.3.1"
  },
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "eject": "react-native eject",
    "test": "jest",
    "release": "react-native run-android --variant=release",
    "link": "react-native link react-native-twitter-signin",
    "unlink": "react-native unlink react-native-twitter-signin",
    "postinstall": "rm -rf node_modules/react-native-twitter-signin/node_modules/react-native && npm run googlenullable && npm run googlenonnull && npm run spinkitnonnull && npm run spinkitnullable && npm run sendintentreplacefile && npm run locationserviceupdate",
    "xcode": "open ios/Boiler.xcworkspace",
    "build:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "googlenullable": "find node_modules/react-native-google-* -type f -exec sed -i '' -e 's/import android.support.annotation.Nullable;/import androidx.annotation.Nullable;/' {} \\;",
    "googlenonnull": "find node_modules/react-native-google-* -type f -exec sed -i '' -e 's/import android.support.annotation.NonNull;/import androidx.annotation.NonNull;/' {} \\;",
    "spinkitnullable": "find node_modules/react-native-spinkit -type f -exec sed -i '' -e 's/import android.support.annotation.Nullable;/import androidx.annotation.Nullable;/' {} \\;",
    "spinkitnonnull": "find node_modules/react-native-spinkit -type f -exec sed -i '' -e 's/import android.support.annotation.NonNull;/import androidx.annotation.NonNull;/' {} \\;",
    "locationserviceupdate": "find node_modules/react-native-geolocation-service -type f -exec sed -i '' -e 's/import android.support.v4.app.ActivityCompat;/import androidx.core.app.ActivityCompat;/' {} \\;",
    "sendintentreplacefile": "find node_modules/react-native-send-intent -type f -exec sed -i '' -e 's/import android.support.v4.content.FileProvider;/import androidx.core.content.FileProvider;/' {} \\;"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/react-native/jest/preprocessor.js"
    }
  },
  "rnpm": {
    "assets": [
      "./src/boiler/assets/fonts/"
    ]
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.3.4",
    "appcenter": "^1.12.2",
    "appcenter-analytics": "^1.12.2",
    "appcenter-crashes": "^1.12.2",
    "axios": "^0.18.0",
    "babel-preset-react-native": "^5.0.2",
    "d3-interpolate": "^1.3.2",
    "firebase": "^5.6.0",
    "install": "^0.13.0",
    "libphonenumber-js": "^1.7.23",
    "lodash": "^4.17.11",
    "metro-react-native-babel-preset": "^0.54.0",
    "moment": "^2.22.2",
    "native-base": "^2.8.0",
    "npm": "^6.10.2",
    "react": "^16.8.3",
    "react-native": "^0.62.3",
    "react-native-action-button": "^2.8.5",
    "react-native-android-location-services-dialog-box": "^2.8.2",
    "react-native-camera": "^3.4.0",
    "react-native-code-push": "^5.5.2",
    "react-native-contacts": "^5.0.3",
    "react-native-content-loader": "^0.1.6",
    "react-native-country-picker-modal": "^0.8.0",
    "react-native-dash": "0.0.9",
    "react-native-device-info": "^2.1.3",
    "react-native-facebook-account-kit": "^2.1.0",
    "react-native-fbsdk": "^1.0.1",
    "react-native-fs": "^2.14.1",
    "react-native-geolocation-service": "^3.1.0",
    "react-native-google-signin": "^2.0.0",
    "react-native-image-crop-picker": "^0.25.0",
    "react-native-image-picker": "^1.0.2",
    "react-native-linear-gradient": "^2.4.3",
    "react-native-linkedin": "^1.3.2",
    "react-native-modal-datetime-picker": "^6.1.0",
    "react-native-phone-input": "^0.2.4",
    "react-native-play-audio": "^0.1.2",
    "react-native-popup-menu": "^0.15.5",
    "react-native-push-notification": "^3.1.8",
    "react-native-render-html": "^4.1.2",
    "react-native-router-flux": "^4.0.1",
    "react-native-scrollable-tab-view": "^0.8.0",
    "react-native-send-intent": "^1.0.30",
    "react-native-share": "^2.0.0",
    "react-native-sms": "^1.9.0",
    "react-native-snap-carousel": "^3.7.4",
    "react-native-spinkit": "^1.3.0",
    "react-native-splash-screen": "^3.2.0",
    "react-native-sqlite-storage": "^4.1.0",
    "react-native-svg": "^9.9.3",
    "react-native-svg-charts": "^5.2.0",
    "react-native-svg-uri": "^1.2.3",
    "react-native-swiper": "^1.5.14",
    "react-native-tab-view": "^1.0.2",
    "react-native-toaster": "^1.2.2",
    "react-native-twitter-signin": "^1.1.1",
    "react-native-vector-icons": "^5.0.0",
    "react-native-view-overflow": "0.0.4",
    "react-native-webview": "^6.9.0",
    "react-navigation": "^2.14.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "rn-fetch-blob": "^0.10.16",
    "toggle-switch-react-native": "^2.0.2"
  }
}
